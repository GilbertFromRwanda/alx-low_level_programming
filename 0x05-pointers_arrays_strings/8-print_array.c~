#include "main.h"

/**
* print_array - function code
*@a: pointer of array
*@n: number of array element
* Return: 0
*/

int main(void)
{
    int array[5];

    array[0] = 98;
    array[1] = 402;
    array[2] = -198;
    array[3] = 298;
    array[4] = -1024;
    print_array(array, 5);
    return (0);
}
void print_array(int *a, int n)
{
int g;
for (g =0; g < n; g++)
{
printf("%d",a[g]);
if (g != n - 1)
{
printf(", ");
}
}
printf("\n");
}
