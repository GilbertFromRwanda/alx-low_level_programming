#include "main.h"

/**
* _strcpy - function code
*@dest: string
*@src: string source
* Return: 0
*/
int main(void)
{
    char s1[98];
    char *ptr;

    ptr = _strcpy(s1, "First, solve the problem. Then, write the code\n");
    printf("%s", s1);
    printf("%s", ptr);
    return (0);
}
char *_strcpy(char *dest, char *src)
{
return (strcpy(dest, src));
}
