#include "main.h"

/**
* print_chessboard - function code
*@s: string array
*@to: string to
* Return: nothing
*/
int main(void)
{
    char *s0 = "Bob Dylan";
    char *s1 = "Robert Allen";

    printf("%s, %s\n", s0, s1);
    set_string(&s1, s0);
    printf("%s, %s\n", s0, s1);
    return (0);
}

void set_string(char **s, char *to)
{
s = &to ;
printf("%d",s);
}
